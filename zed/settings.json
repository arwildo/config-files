{
  "languages": {
    "Plain Text": {
      "enable_language_server": false
    },
    "CSS": {
      "enable_language_server": false
    }
  },
  "agent": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4.1"
    },
    "model_parameters": []
  },
  "base_keymap": "VSCode",
  "vim_mode": true,
  "ui_font_size": 13,
  "buffer_font_size": 13,
  "diagnostics": {
    "enabled": false
  },
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "format_on_save": "off",
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark"
  }
}
