{
  "base_keymap": "VSCode",
  "vim_mode": true,
  "ui_font_size": 13,
  "buffer_font_size": 13,
  "diagnostics": {
    "enabled": false
  },
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "format_on_save": "off",
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark"
  }
}

